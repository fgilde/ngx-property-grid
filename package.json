{
  "name": "ngx-property-grid",
  "description": "A small and simple property grid in angular to view/edit POJOs, excellent if you have a \"settings\" object you want to give the user to edit.",
  "version": "9.0.6",
  "private": false,
  "authors": [
    "fgilde",
    "mokeyish"
  ],
  "keywords": [
    "angular",
    "property grid",
    "ngx",
    "angular9"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgilde/ngx-property-grid.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-lib-prod": "ng serve --configuration=lib-prod",
    "build": "ng build",
    "build-lib": "ng build ngx-property-grid",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "publish-npm": "npm run build-lib && npm publish dist/ngx-property-grid --access public",
    "doc:build": "./node_modules/.bin/compodoc -p projects/ngx-property-grid/tsconfig.lib.json -d ./docs && ng build --aot --output-path docs/demo --base-href /demo/",
    "doc:buildandserve": "./node_modules/.bin/compodoc -p projects/ngx-property-grid/tsconfig.lib.json -d ./docs -s"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.6",
    "@angular/core": "^17.0.6"
  },
  "dependencies": {
    "@fgilde/ngx-template": "^1.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.6",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/animations": "~17.0.6",
    "@angular/cdk": "^17.0.2",
    "@angular/cli": "~17.0.6",
    "@angular/common": "~17.0.6",
    "@angular/compiler": "~17.0.6",
    "@angular/compiler-cli": "~17.0.6",
    "@angular/core": "~17.0.6",
    "@angular/forms": "~17.0.6",
    "@angular/language-service": "~17.0.6",
    "@angular/material": "^17.0.2",
    "@angular/platform-browser": "~17.0.6",
    "@angular/platform-browser-dynamic": "~17.0.6",
    "@angular/router": "~17.0.6",
    "@compodoc/compodoc": "^1.1.23",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^20.10.4",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-json-view": "^4.0.0",
    "ng-packagr": "^17.0.2",
    "protractor": "~7.0.0",
    "rxjs": "~7.8.1",
    "ts-node": "~10.9.1",
    "tslib": "^2.6.2",
    "tslint": "~6.1.3",
    "typescript": "~5.2.2",
    "zone.js": "~0.14.2"
  }
}
